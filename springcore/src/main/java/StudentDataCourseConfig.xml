<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean class="com.jspiders.springcore.bean.StudentDataBean"
		name="student1">
		<property name="id" value="1" />
		<property name="name" value="ramesh" />
		<property name="email" value="ramesh@gmail.com" />
		<property name="courses">
			<list>
				<bean class="com.jspiders.springcore.bean.CourseBean"
					name="course1">
					<property name="id" value="1" />
					<property name="name" value="CoreJava" />
					<property name="fees" value="15000" />
				</bean>

				<bean class="com.jspiders.springcore.bean.CourseBean"
					name="course2">
					<property name="id" value="2" />
					<property name="name" value="SQL" />
					<property name="fees" value="10000" />
				</bean>

				<bean class="com.jspiders.springcore.bean.CourseBean"
					name="course3">
					<property name="id" value="3" />
					<property name="name" value="WebTech" />
					<property name="fees" value="10000" />
				</bean>
			</list>
		</property>
	</bean>

	<bean class="com.jspiders.springcore.bean.StudentDataBean"
		name="student2">
		<property name="id" value="2" />
		<property name="name" value="suresh" />
		<property name="email" value="suresh@gmail.com" />
		<property name="courses">
			<list>
				<bean class="com.jspiders.springcore.bean.CourseBean"
					name="course1">
					<property name="id" value="1" />
					<property name="name" value="CoreJava" />
					<property name="fees" value="15000" />
				</bean>

				<bean class="com.jspiders.springcore.bean.CourseBean"
					name="course2">
					<property name="id" value="2" />
					<property name="name" value="SQL" />
					<property name="fees" value="10000" />
				</bean>


</list>
</property>
</bean>
</beans>
